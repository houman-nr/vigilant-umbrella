        "import pandas as pd\n",
        "\n",
        "with open('SST2_Train.json', 'r') as file:\n",
        "    loaded_data = json.load(file)\n",
        "data_df = pd.DataFrame(loaded_data)\n",
        "\n",
        "data_df.head()"
      ],
